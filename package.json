{
  "name": "mutambo-web",
  "version": "2.0.0",
  "description": "https://mutambo.com",
  "repository": "git@github.com:waterstone15/mutambo-web.git",
  "author": "Brian Waterstone <code@waterstone15.com>",
  "license": "The Unlicense",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "build:copy": "cp ./vercel.json ./dist/vercel.json && cp -R ./.vercel ./dist",
    "build:prod": "yarn build:tailwind:prod && ember build -prod && yarn build:copy",
    "build:tailwind": "tailwindcss -i ./app/styles/tw.css -o ./app/styles/tw-out.css",
    "build:tailwind:prod": "tailwindcss -i ./app/styles/tw.css -o ./app/styles/tw-out.css",
    "deploy": "yarn build:prod && cd ./dist && vc",
    "deploy:prod": "yarn build:prod && cd ./dist && vc --prod",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "s": "npm-run-all --parallel s:ember s:tailwind",
    "s:build": "serve -s ./dist -l 5021",
    "s:ember": "ember serve --p 5021",
    "s:tailwind": "tailwindcss -i ./app/styles/tw.css -o ./app/styles/tw-out.css --watch",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test"
  },
